{
  "name": "gatsby-starter-mdx-website-blog",
  "private": true,
  "description": "A starter website and blog powered by Gatsby and Markdown with MDX",
  "version": "0.1.0",
  "author": "Doak Heggeness <doakheggeness@gmail.com>",
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.4.0",
    "@mdx-js/react": "1.4.0",
    "@mdx-js/tag": "0.20.3",
    "gatsby": "2.20.2",
    "gatsby-cli": "2.11.0",
    "gatsby-image": "2.3.0",
    "gatsby-plugin-emotion": "4.2.0",
    "gatsby-plugin-feed": "2.4.0",
    "gatsby-plugin-google-analytics": "2.2.0",
    "gatsby-plugin-manifest": "2.3.1",
    "gatsby-plugin-mdx": "1.1.0",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-react-helmet": "3.2.0",
    "gatsby-plugin-sharp": "2.5.1",
    "gatsby-plugin-sitemap": "2.3.0",
    "gatsby-remark-copy-linked-files": "2.2.0",
    "gatsby-remark-images": "3.2.0",
    "gatsby-remark-prismjs": "3.4.0",
    "gatsby-remark-responsive-iframe": "2.3.0",
    "gatsby-remark-smartypants": "2.2.0",
    "gatsby-source-filesystem": "2.2.1",
    "gatsby-transformer-remark": "2.7.0",
    "gatsby-transformer-sharp": "2.4.1",
    "mdx-utils": "0.2.0",
    "prismjs": "1.19.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-icons": "3.9.0",
    "splitting": "^1.0.6",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typeface-raleway": "0.0.75"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.14.3",
    "prettier": "1.19.1"
  },
  "homepage": "https://gatsby-starter-mdx-website-blog.netlify.com/",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doakheggeness/gatsby-starter-mdx-website-blog.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
